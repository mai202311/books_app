<!DOCOTYPE html>
<html lang="ja">
    <meta charset="UTF-8" />
    
<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
<%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    
<body>
 <head>
<title>WebCampBackendNew</title>
 </head>
 
 <h1>Books</h1>
 
 <%= @lists.each do |list| %>

<tr>
   <td>Title</td>
      <span><%= list.title %></span>
   <td>Body</td>
      <span><%= list.body %></span>
   <td colspan="3"></td>
 </tr>
 <% end %>

<h1>New book</h1>
<!-- form_with部分 -->
<%= form_with model: Book.new do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
	
  <h4>Body</h4>
  <%= f.text_area :body %>
	
  <%= f.submit 'Create Book' %>
<% end %>


   
</body>
</html>